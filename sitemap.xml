---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for post in site.posts %}
    <url>
        <loc>http://www.toubsen.de{{ post.url | remove: 'index.html' }}</loc>
        <lastmod>{{ post.date | date:"%F" }}</lastmod>
    </url>
    {% endfor %}

    {% for page in site.pages %}
    {% if page.layout != null %}
    {% if page.layout != 'feed' %}
    {% if page.sitemap != 'hidden' %}
    <url>
        <loc>http://www.toubsen.de{{ page.url | remove: 'index.html' }}</loc>
        <lastmod>{{ site.time | date:"%F" }}</lastmod>
    </url>
    {% endif %}
    {% endif %}
    {% endif %}
    {% endfor %}
</urlset>
